{
  "name": "mindustry-schematic-parser",
  "version": "4.2.0",
  "description": "A simple api to parse mindustry schematics",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "start": "node .",
    "test": "uvu -r tsm src \\.test\\.ts",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepublishOnly": "pnpm run build"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.mjs",
    "default": "./dist/index.mjs"
  },
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "Mindustry"
  ],
  "author": "JeanJPNM",
  "license": "ISC",
  "packageManager": "pnpm@7.0.0",
  "dependencies": {
    "canvas": "^2.10.2",
    "pako": "^2.1.0",
    "pkg-dir": "^7.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/node": "^14.17.0",
    "@types/pako": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "rollup": "^2.67.3",
    "tsm": "^2.2.1",
    "typescript": "^4.5.5",
    "uvu": "^0.5.3"
  },
  "files": [
    "assets/",
    "CHANGELOG.md",
    "README.md",
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JeanJPNM/mindustry-schematic-parser.git"
  },
  "bugs": {
    "url": "https://github.com/JeanJPNM/mindustry-schematic-parser/issues"
  },
  "homepage": "https://github.com/JeanJPNM/mindustry-schematic-parser#readme"
}
